# backend/core/institutional_detector.py

import numpy as np
from typing import Dict, Optional, Tuple
from dataclasses import dataclass

@dataclass
class InstitutionalSignal:
    """Detected institutional activity"""
    confidence: float  # 0-1
    volume_usd: float
    price_stability: float
    cross_exchange_correlation: float
    signal_type: str  # "accumulation", "distribution", "neutral"
    description: str


class InstitutionalWallDetector:
    """
    Detects large institutional buy/sell walls using manifold geometry.
    
    Looks for:
    - High volume concentration
    - Low price volatility (pinning)
    - Cross-exchange coherence
    - Order book depth anomalies
    """
    
    def __init__(
        self,
        min_wall_size_usd: float = 100_000_000,  # $100M
        volatility_threshold: float = 0.10,  # Bottom 10% volatility
        correlation_threshold: float = 0.85  # 85% cross-exchange agreement
    ):
        self.min_wall_size = min_wall_size_usd
        self.volatility_threshold = volatility_threshold
        self.correlation_threshold = correlation_threshold
    
    def detect(
        self,
        volume_usd: float,
        price_data: np.ndarray,
        orderbook_depth: Optional[Dict] = None,
        exchange_data: Optional[Dict] = None
    ) -> InstitutionalSignal:
        """
        Analyze market data for institutional activity.
        
        Args:
            volume_usd: Total volume in USD
            price_data: Price array for volatility calculation
            orderbook_depth: Optional order book data
            exchange_data: Optional multi-exchange data
        
        Returns:
            InstitutionalSignal with confidence and details
        """
        
        # 1. Check volume threshold
        is_mass_heavy = volume_usd > self.min_wall_size
        
        # 2. Calculate price stability (inverse of volatility)
        volatility = self._calculate_volatility(price_data)
        historical_vol = np.percentile(volatility, self.volatility_threshold * 100)
        is_pinned = volatility[-1] < historical_vol
        
        # 3. Check cross-exchange correlation if available
        correlation = 0.0
        if exchange_data:
            correlation = self._calculate_cross_exchange_correlation(exchange_data)
        
        # 4. Analyze order book depth if available
        depth_score = 0.0
        if orderbook_depth:
            depth_score = self._analyze_orderbook_depth(orderbook_depth)
        
        # Calculate confidence
        confidence = self._calculate_confidence(
            is_mass_heavy,
            is_pinned,
            correlation,
            depth_score
        )
        
        # Determine signal type
        signal_type = self._classify_signal(
            is_mass_heavy,
            is_pinned,
            orderbook_depth
        )
        
        # Generate description
        description = self._generate_description(
            confidence,
            signal_type,
            volume_usd,
            volatility[-1]
        )
        
        return InstitutionalSignal(
            confidence=confidence,
            volume_usd=volume_usd,
            price_stability=1.0 - (volatility[-1] / np.max(volatility)),
            cross_exchange_correlation=correlation,
            signal_type=signal_type,
            description=description
        )
    
    def _calculate_volatility(self, prices: np.ndarray) -> np.ndarray:
        """Calculate rolling volatility"""
        returns = np.diff(prices) / prices[:-1]
        # Rolling std over 20 periods
        window = 20
        volatility = np.array([
            np.std(returns[max(0, i-window):i+1])
            for i in range(len(returns))
        ])
        return volatility
    
    def _calculate_cross_exchange_correlation(
        self,
        exchange_data: Dict[str, np.ndarray]
    ) -> float:
        """
        Calculate correlation between exchanges.
        High correlation = coordinated movement = institutional
        """
        if len(exchange_data) < 2:
            return 0.0
        
        exchanges = list(exchange_data.keys())
        correlations = []
        
        for i in range(len(exchanges)):
            for j in range(i+1, len(exchanges)):
                data1 = exchange_data[exchanges[i]]
                data2 = exchange_data[exchanges[j]]
                
                # Ensure same length
                min_len = min(len(data1), len(data2))
                data1 = data1[-min_len:]
                data2 = data2[-min_len:]
                
                # Calculate correlation
                corr = np.corrcoef(data1, data2)[0, 1]
                correlations.append(corr)
        
        return np.mean(correlations)
    
    def _analyze_orderbook_depth(self, orderbook: Dict) -> float:
        """
        Analyze order book for iceberg orders (hidden institutional walls).
        
        Looks for:
        - Large bid/ask concentrations
        - Unusual depth at specific price levels
        - Sudden depth changes (walls appearing/disappearing)
        """
        bids = orderbook.get('bids', [])
        asks = orderbook.get('asks', [])
        
        if not bids or not asks:
            return 0.0
        
        # Calculate depth concentration
        # Iceberg orders show up as unusually large depth at specific levels
        bid_volumes = [float(bid[1]) for bid in bids[:50]]  # Top 50 levels
        ask_volumes = [float(ask[1]) for ask in asks[:50]]
        
        # Check for outliers (walls)
        bid_mean = np.mean(bid_volumes)
        bid_std = np.std(bid_volumes)
        
        # Count levels with > 3 std deviation (institutional walls)
        wall_count = sum(1 for vol in bid_volumes if vol > bid_mean + 3*bid_std)
        
        # Score: 0-1 based on wall presence
        depth_score = min(wall_count / 5.0, 1.0)  # Max 5 walls = score of 1.0
        
        return depth_score
    
    def _calculate_confidence(
        self,
        is_mass_heavy: bool,
        is_pinned: bool,
        correlation: float,
        depth_score: float
    ) -> float:
        """
        Calculate confidence that this is institutional activity.
        
        Weights:
        - Volume: 40%
        - Stability: 30%
        - Correlation: 20%
        - Depth: 10%
        """
        score = 0.0
        
        if is_mass_heavy:
            score += 0.4
        
        if is_pinned:
            score += 0.3
        
        score += correlation * 0.2
        score += depth_score * 0.1
        
        return min(score, 1.0)
    
    def _classify_signal(
        self,
        is_mass_heavy: bool,
        is_pinned: bool,
        orderbook: Optional[Dict]
    ) -> str:
        """Classify the type of institutional activity"""
        
        if not is_mass_heavy:
            return "neutral"
        
        if is_pinned:
            # Check if more bids or asks (accumulation vs distribution)
            if orderbook:
                bid_depth = sum(float(b[1]) for b in orderbook.get('bids', [])[:20])
                ask_depth = sum(float(a[1]) for a in orderbook.get('asks', [])[:20])
                
                if bid_depth > ask_depth * 1.5:
                    return "accumulation"  # Buying pressure
                elif ask_depth > bid_depth * 1.5:
                    return "distribution"  # Selling pressure
            
            return "accumulation"  # Default to accumulation if pinned
        
        return "neutral"
    
    def _generate_description(
        self,
        confidence: float,
        signal_type: str,
        volume_usd: float,
        volatility: float
    ) -> str:
        """Generate human-readable description"""
        
        if confidence > 0.8:
            strength = "STRONG"
        elif confidence > 0.6:
            strength = "MODERATE"
        elif confidence > 0.4:
            strength = "WEAK"
        else:
            return "No significant institutional activity detected"
        
        return (
            f"{strength} institutional {signal_type.upper()} detected. "
            f"${volume_usd/1e6:.1f}M volume with {volatility*100:.2f}% volatility. "
            f"Confidence: {confidence*100:.0f}%"
        )


